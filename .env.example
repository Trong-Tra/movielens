PORT=3001
NODE_ENV=development
MOVIELENS_DATASET_PATH=./data/ml-1m

# Model Caching
# Set to 'false' to force model retraining on every startup
# When 'true', trained models are saved to data/models/ and loaded from cache
# Default: true (recommended for faster startup - models load in <1s instead of ~90s)
USE_MODEL_CACHE=true
